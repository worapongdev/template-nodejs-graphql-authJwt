{
  "name": "template-nodejs-graphql",
  "version": "1.0.0",
  "description": "config nodejs include dependencies graphql postgres sequelize",
  "main": "index.js",
  "repository": "https://github.com/worapongdev/template-nodejs-graphql.git",
  "author": "worapong changsan <worapongdev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "db:migrate:gen": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:status": "sequelize db:migrate:status",
    "db:seed:generate": "sequelize seed:generate --name ",
    "db:seed:undo": "sequelize db:seed:undo",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "db:model:generate": "sequelize model:generate"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.11.5",
    "apollo-server": "^2.18.2",
    "apollo-server-express": "^2.18.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "14.7.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.4",
    "pg": "^8.4.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "regenerator-runtime": "^0.13.7",
    "sequelize-cli": "^6.2.0"
  }
}
